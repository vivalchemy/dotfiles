#git porcelain aliases
alias g='git'
alias ga='git add --verbose'
alias gb='git branch'
alias gc='git commit'
alias gco='git checkout'
alias gl='git -c core.pager="bat" log --no-color'
alias glg='git log --graph  --oneline --all'
alias glo='git log --oneline'
alias gp='git pull --no-edit'
alias gP='git push'
alias gr='git remote'
alias gs='git switch'
